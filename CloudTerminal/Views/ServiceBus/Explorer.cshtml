<div ng-controller="AppServiceController" ng-init="InitializeController()">
    <div class="row" style="margin-top:10px" ng-init="GetAllQueue()">
        <div class=" col-lg-3 col-md-6 ng-isolate-scope">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-3">
                            <i class="fa icon-azure-icon-service-bus-queue fa-5x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <div class="huge ng-binding">{{QueueList.length}}</div>
                            <div class="ng-binding">Queues</div>
                        </div>
                    </div>
                </div>
                <a ui-sref="home.Dashboard.ServiceBus.Explorer.Queues">
                    <div class="panel-footer">
                        <span class="pull-left">View Details</span>
                        <span class="pull-right">
                            <i class="fa fa-arrow-circle-right"></i>
                        </span>
                        <div class="clearfix"></div>
                    </div>
                </a>
            </div>
        </div>

        <div class=" col-lg-3 col-md-6 ng-isolate-scope" ng-init="GetAllTopics()">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-3">
                            <i class="fa icon-azure-icon-service-bus-topics-and-subscriptions fa-5x"></i>
                        </div>
                        <div class="col-xs-9 text-right">
                            <div class="huge ng-binding">{{TopicList.length}}</div>
                            <div class="ng-binding">Topics</div>
                        </div>
                    </div>
                </div>
                <a ui-sref="home.Dashboard.ServiceBus.Explorer.Topics">
                    <div class="panel-footer">
                        <span class="pull-left">View Details</span>
                        <span class="pull-right">
                            <i class="fa fa-arrow-circle-right"></i>
                        </span>
                        <div class="clearfix"></div>
                    </div>
                </a>


            </div>

        </div>
    </div>

    </div>
