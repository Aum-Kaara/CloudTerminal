        <div class="container">
            <div class="row">
                <div class="col-sm-10">
                    <button class="btn btn-lg color-facebook" data-toggle="tooltip" title="" data-original-title="Follow on Facebook"><span class="fa fa-facebook fa-lg"></span></button>
                    <button class="btn btn-lg color-google" data-toggle="tooltip" title="" data-original-title="Follow on Google+"><span class="fa fa-google-plus fa-lg"></span></button>
                    <button class="btn btn-lg color-twitter" data-toggle="tooltip" title="" data-original-title="Follow on Twitter"><span class="fa fa-twitter fa-lg"></span></button>
                    <button class="btn btn-lg color-pinterest" data-toggle="tooltip" title="" data-original-title="Follow on Pinterest"><span class="fa fa-pinterest fa-lg"></span></button>
                    <button class="btn btn-lg color-tumblr" data-toggle="tooltip" title="" data-original-title="Follow on Tumblr"><span class="fa fa-tumblr fa-lg"></span></button>
                </div>

            </div>
        </div>
    </footer>*@
   
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrap-toggle.min.js"></script>
    <script src="~/Scripts/angular-ui-router.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="~/Scripts/angular-block-ui.js"></script>
    <script src="~/Scripts/angular-inform.min.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls-0.12.0.min.js"></script>
    <script src="~/Scripts/ngBreadCrumb.js"></script>
    <script src="~/Scripts/contextMenu.js"></script>
    <script src="~/scripts/app.js"></script>
    <script src="~/Scripts/AzureFactory.js"></script>
    <script src="~/scripts/MasterController.js"></script>
    <script src="~/Scripts/DataServiceController.js"></script>
    <script src="~/Scripts/AppServiceController.js"></script>
    


    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href=" https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.3/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/dashboard.css" rel="stylesheet" />
    <link href="~/Content/sb-admin-2.css" rel="stylesheet" />
    <link href="http://angular-block-ui.nullest.com/angular-block-ui/angular-block-ui.css" rel="stylesheet" />
    <link href="~/Content/angular-inform.min.css" rel="stylesheet" />
    <link href="~/Content/azureiconset.css" rel="stylesheet" />
