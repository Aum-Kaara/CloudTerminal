<html>
<head>
    <title></title>

    <base href="/" />

</head>

<body ng-app="AzureApp" ng-controller="MasterController">
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button aria-controls="navbar" aria-expanded="false" data-target="#navbar" data-toggle="collapse" class="navbar-toggle collapsed" type="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <a href="#" class="navbar-brand">

                    <i class="glyphicon glyphicon-cloud"></i> Cloud
                    Terminal <i class="fa fa-plane fa-fw"></i>
                </a>
                <span class="glyphicon glyphicon-cloud" aria-hidden="true"></span>
            </div>
            <div class="navbar-collapse collapse" id="navbar">
                <ul class="nav navbar-nav navbar-right">
                    <li><a ui-sref="home.Features">Features</a></li>
                    <li><a href="#/Dashboard">Dashboard</a></li>
                    <li><a href="#">Help</a></li>

                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" aria-expanded="false">
                            <i class="fa fa-user fa-fw"></i>
                            <i class="fa fa-caret-down"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#/UserProfile">
                                    <i class="fa fa-user fa-fw"></i> User Profile
                                </a>
                            </li>
                            <li>
                                <a href="#" data-target="#commonModalDialog" data-toggle="modal">
                                    <i class="fa fa-Envelope fa-fw"></i> Connect Us
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <i class="fa fa-gear fa-fw"></i> Settings
                                </a>
                            </li>

                            <li class="divider"></li>
                            <li>
                                @if (!Request.IsAuthenticated)
                                {
                                    <a ui-sref="home.Login">
                                        <i class="fa fa-sign-in fa-fw"></i> Login
                                    </a>
                                }
                                @if (Request.IsAuthenticated)
                                {
                                    <a href='@Url.Action("SignOut", "Account")'>
                                        <i class="fa fa-sign-out fa-fw"></i> Logout
                                    </a>
                                }
                            </li>
                        </ul>
                        <!-- /.dropdown-user -->
                    </li>
                </ul>
                <form class="navbar-form navbar-right">
                    <input type="text" placeholder="Search..." class="form-control">
                </form>
            </div>
        </div>
    </nav>
    
    <div block-ui="main" class="block-ui-main">
        <div class="breadcrumb">
            <div>
                <ui-breadcrumbs displayname-property="data.displayName" abstract-proxy-property="data.proxy" template-url="Home\BreadCrumb"></ui-breadcrumbs>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row">
                               <div class="col-sm-3  col-md-12">

                    <div ui-view="main">

                    </div>

                </div>

            </div>
        </div>

        <div tabindex="-1" class="modal in" id="commonModalDialog" role="dialog" aria-hidden="false" aria-labelledby="myModalLabel" style="display: none;" data-keyboard="false" data-backdrop="static">
            <div class="modal-dialog" id="commonModalDialogBox">
                <div class="modal-content" id="commonModalDialogContent" style="top: 90.5px;">
                    <div class="modal-header">
                        <button title="Close" class="close" id="btnXViewDatabaseServer" aria-hidden="true" type="button" data-dismiss="modal">×</button>
                        <h4 class="modal-title" id="myModalLabel">Get In Touch with us.</h4>
                    </div>
                    <div class="modal-body">
                        Share your feedback with us on mohit.e.gupta@gmail.com
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-link" type="button" data-dismiss="modal">
                            <i class="fa fa-remove fa-2x"></i>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
